{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "quotes": "off",
        "max-len": "off",
        "arrow-parens": "off",
        "indent": "off",
        "comma-dangle": "off",
        "eol-last": ["error", "never"]

        
    },
    "extends": ["eslint:recommended", "google"],
}